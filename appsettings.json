{
  "AppSettings": {
    "Token": "$TOKEN"
  },
  "ConnectionStrings": {
     "DefaultConnection": "Data Source=$DATASOURCE"
    "DefaultConnection": "Server = $SERVER; Database = $DATABASE; Uid = $UID; Pwd = $PWD"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CloudinaryCredentials": {
    "CloudName": "$CLOUDNAME",
    "ApiKey": "$APIKEY",
    "ApiSecret": "$APISECRET"
  }
}
